/* Description:
 * Check if a sentence is palindromic.
 */

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main(int argc, char *argv[])
{
  char *sentence = NULL;
  scanf("%ms", &sentence);
  printf("%s\n", sentence);
  return 0;
}
